msgid "The Format -Title 2- (H2 HTML tags) inside pages will be submenu."
msgstr "Les Format -Titre 2- (balises HTML H2) des pages seront des sous-menu."

#: ../../../scrollnav.php:20
msgid "Parameters :"
msgstr "Paramètres :"

#: ../../../scrollnav.php:23
msgid "Scrollnav Version :"
msgstr "Version de Scrollnav :"

#: ../../../scrollnav.php:26
msgid "Version 2 - JQuery Required"
msgstr "Version 2 - N&eacute;c&eacute;ssite JQuery"

#: ../../../scrollnav.php:27
msgid "Version 3 - Pure JavaScript, less options"
msgstr "Version 3 - Pur JavaScript, moins d'options"

#: ../../../scrollnav.php:30
